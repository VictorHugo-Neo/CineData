generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model movies {
  id_titulo        String  @id
  titulo_original  String?
  ano_lancamento   Int?
  genero           String?
  nota_media       Float?
  numero_votos     Int?
  genero_principal String?
  score_ranking    Float?
}